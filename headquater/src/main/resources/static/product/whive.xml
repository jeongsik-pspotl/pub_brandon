		<script lazy="false" type="text/javascript"><![CDATA[const isMobile = window.matchMedia('screen and (hover: none), screen and (pointer: coarse)').matches;

scwin.onpageload = function() {
	$p.top().grp_wrap.addClass("sub_pd");
    scwin.referenceSwp();
    scwin.iconSwiper();
    scwin.msSwiper();
    AOS.init();    
};


scwin.referenceSwp = function(){
    if(!isMobile){
        var referenceSwp = new Swiper(".reference_list ", {
            slidesPerView: 3,
            spaceBetween: 40,
            navigation: {
                nextEl:".swiper-button-next",
                prevEl:".swiper-button-prev",
            },
        });
    }else{
        var referenceSwp = new Swiper(".reference_list ", {
            slidesPerView: 1,
            spaceBetween: 40,
            navigation: {
                nextEl:".swiper-button-next",
                prevEl:".swiper-button-prev",
            },
        });
    }
}

scwin.iconSwiper = function() {
    if(isMobile){
        var iconSwiper = new Swiper(".icon_swiper", {
            slidesPerView: "auto",
            spaceBetween: 16,
            freeMode: true,
            mousewheel: true
        });
    }
}

scwin.msSwiper = function() {
    if(isMobile){
        var msSwiper = new Swiper(".ms_swiper", {
            slidesPerView: "auto",
            spaceBetween: 20,
            centeredSlides: true,
            pagination: {
                el: ".swiper-pagination",
            },
        });
    }
}]]></script>
