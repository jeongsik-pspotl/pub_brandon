        <w2:layoutInfo/>
        <w2:publicInfo method=""/>
        <script lazy="false" type="text/javascript"><![CDATA[

            scwin.btn_project_step1_checkYn = false;
            scwin.btn_project_step2_checkYn = false;
            scwin.btn_project_step3_checkYn = false;
            scwin.btn_project_step4_checkYn = false;

            scwin.txt_project_all_step_platform = "Android";
            scwin.txt_project_all_step_project_name = "";

            const message_warning = common.getLabel("lbl_project_add_js_warning");

            scwin.paramData = {};

            scwin.onpageload = function() {
                scwin.paramData = $p.getParameter("tabParam");
                scwin.selected_step(1);
            };

            scwin.selected_step = function(selected){
                var jsonData = dtl_steps.getAllJSON();
                for(var obj in jsonData){
                    var row = jsonData[obj];
                    if(row.INDEX == selected){
                        var temp = $p.getComponentById(row.FRAMEID);
                        if(temp.getSrc() == ""){
                            var obj = {
                                "dataObject": {
                                    "type" : "json",
                                    "name" : "wframeParam",
                                    "data" : scwin.paramData
                                }
                            };
                            temp.setSrc(row.FRAMESRC, obj);
                        }
                        $p.getComponentById(row.FRAMEID).show();
                    } else {
                        $p.getComponentById(row.FRAMEID).hide();
                    }
                }
            };

]]></script>
