				<w2:dataList baseNode="list" repeatNode="map" id="dtl_ios_debug_profile" saveRemovedData="true">
					<w2:columnInfo>
						<w2:column id="name" name="name" dataType="text"></w2:column>
						<w2:column id="value" name="value" dataType="text"></w2:column>
					</w2:columnInfo>
